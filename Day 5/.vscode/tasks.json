{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Clang Build",
      "type": "shell",
      "command": "clang++",
      "args": [
        "-std=c++17",
        "-stdlib=libc++",
        "Day 5.cpp",
        "-o",
        "Day5.out",
        "--debug"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "msvc build",
      "type": "shell",
      "command": "cl.exe",
      "args": ["/EHsc", "/Zi", "/Fe:", "Day5.exe", "Day 5.cpp"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always"
      },
      "problemMatcher": "$msCompile"
    }
  ]
}
